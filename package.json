{
  "name": "ami-bridge",
  "version": "2.0.5",
  "description": "Asterisk Manager Interface AMI Client",
  "keywords": [
    "asterisk",
    "manager",
    "ami"
  ],
  "homepage": "https://github.com/savagz/ami-bridge#readme",
  "bugs": {
    "url": "https://github.com/savagz/ami-bridge/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savagz/ami-bridge.git"
  },
  "license": "MIT",
  "author": "Samuel Vargas (savagz)",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ami-bridge": "dist/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "pretest": "npm run build",
    "test": "vitest run",
    "coverage:lcov": "vitest run --coverage",
    "prepare": "npm run build",
    "coveralls": "echo \"Deprecated. Use coverage:lcov and Codecov in CI.\" && exit 1"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^1.5.0",
    "vitest": "^1.5.0",
    "eslint": "^9.0.0",
    "typescript-eslint": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.9.0"
  },
  "engines": {
    "node": ">=18"
  }
}
